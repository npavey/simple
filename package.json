{
  "name": "simple",
  "version": "3.1.3",
  "description": "This template is a basic course format",
  "main": "gulpfile.js",
  "scripts": {
    "precommit": "npm run lint",
    "lint": "node_modules/.bin/eslint -c .eslintrc app/",
    "preinstall": "npx npm-force-resolutions",
    "postinstall": "node_modules/.bin/bower install",
    "start": "node_modules/.bin/gulp webserver",
    "build": "node_modules/.bin/gulp build",
    "resources": "call easyresources generate --worksheetName \"Simple course\" --outputPath \"lang\" --excludeLocalizations \" \"",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/easygenerator/simple.git"
  },
  "author": "easygenerator",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/easygenerator/simple/issues"
  },
  "homepage": "https://github.com/easygenerator/simple",
  "devDependencies": {
    "del": "^1.0.0",
    "eslint": "^3.16.1",
    "event-stream": "^3.1.7",
    "gulp": "^3.8.8",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-bower": "0.0.10",
    "gulp-clean-css": "^2.0.12",
    "gulp-csso": "^1.0.0",
    "gulp-durandal": "^1.1.5",
    "gulp-has": "^1.0.0",
    "gulp-if": "^2.0.2",
    "gulp-less": "^3.0.5",
    "gulp-load-plugins": "^1.0.0-rc.1",
    "gulp-plumber": "^1.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "gulp-uglify": "^1.0.1",
    "gulp-useref": "^1.0.2",
    "gulp-util": "^3.0.1",
    "gulp-webserver": "^0.9.1",
    "husky": "^0.13.1"
  },
  "dependencies": {
    "gulp-install": "^1.1.0"
  },
  "resolutions": {
    "graceful-fs": "^4.2.4"
  }
}
